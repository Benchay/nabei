.otherManagement{
    margin:0 30px;
    background: #fff;
    padding: 20px 40px;
    height:77%;
    .bankTitle{
        padding: 0 10px;
        text-align: left;
        font-size: 14px;
    }
    .bankCard{
        ul{
            justify-content: flex-start;
            li{
                width: 320px;
                height: 120px;
                border:1px solid #ddd;
                border-radius: 8px;
                justify-content: flex-start;
                position: relative;
                font-size: 14px;
                margin-right: 10px;
                img{
                    margin: 0 15px 0 20px;
                }
                .delete{
                    display: none;
                    position: absolute;
                    right:0;
                    top: 0;
                    font-size: 12px;
                    color:@red;
                    margin: 0;
                    border-radius: 0 8px 0 0;
                }
            }
            .addBank{
                justify-content: center;
                img{
                    width: 40px;
                    margin: 0 10px 0 0 ;
                }
                a{color:#333;}
            }
            li:hover .delete{
                display: block;
            }
            li:hover{
                box-shadow: 1px 1px 3px #ccc;

            }
        }
        .tit{
            font-size: 16px;
            p:first-child{
                padding-bottom: 10px;

            }
        }
    }
    .otherList{
        border-bottom: 1px dashed #ddd;
        padding: 20px 0;
    }
}
.dialogAddBank{
    ul>li{
        width:20%;
        float: left;
        height: 40px;
        line-height: 40px;
        text-align: center;
        margin-bottom: 10px;
        button{
            width:90%;
            border-radius: 5px;
            border:1px solid #d9d9d9;
            div{
                height:100%;
            }

        }
        button{
            width:90%;
            height:100%;
        }
    }
    .user-defined{
        overflow: auto;
        width:100%;
        input{
            width: 90%;
        }
    }
}
.dialogZH{
    overflow: hidden;
    width:100%;
    padding-top: 26px;
    margin-top: 28px;
    border-top:1px solid #d9d9d9 ;
    .ZHtitle{
        text-align: right;
        margin-bottom: 0;
    }
}
.dialogZH>div{
    margin-bottom: 20px;
}
.dialogZH1{
    overflow: hidden;
    width:100%;
    margin-top: 28px;
    div{
        margin-bottom: 10px;
    }
    .wechatTip{
        color:@red;
        font-size: 12px;
        margin-left: 10px;
    }
}
.avatar-uploader .el-upload {
    border: 1px dashed #d9d9d9;
    border-radius: 6px;
    cursor: pointer;
    position: relative;
    overflow: hidden;
}
.avatar-uploader .el-upload:hover {
    border-color: #20a0ff;
}
.avatar-uploader-icon {
    font-size: 28px;
    color: #8c939d;
    width: 128px;
    height: 128px;
    line-height: 128px;
    text-align: center;
}
.avatar {
    width: 128px;
    height: 128px;
    display: block;
}
