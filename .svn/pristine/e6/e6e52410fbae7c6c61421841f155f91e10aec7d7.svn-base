.homeStall{
    color:#333;
    background: #fff;
    margin: 15px 30px;
    padding: 10px 14px;
    height:100%;
    font-size: 12px;
    .isLeft{
        .neweller{
            margin-top: 5px;
        }
        .shortcut{
            font-size: 24px;
            margin-top: 20px;
            ul{
                li{
                    width: 300px;
                    height: 120px;
                    border-radius: 5px;
                    margin-right: 40px;
                    img{
                        margin-right: 35px;
                    }
                    a{
                        color:#fff;
                        width:100%;
                        height:100%;
                    }
                }
                li:hover{
                    box-shadow: 1px 1px 4px #ccc;
                }
                li:first-child{
                    background: #7ec857;
                }
                li:last-child{
                    background: #5d6977;
                }

            }
        }
        .deliveryRecord{
            margin-top: 20px;
            img{
                margin-right: 10px;
            }
            ul{
                margin-left: 40px;
                li{
                    margin: 0 15px;
                    a{
                        color:#666;
                    }
                }
            }
            .checked{
                a{
                    color:@blue;
                }
            }
            .isHover>li>a:hover{
                color:@blue;
            }
        }
        .RecordTable{
            margin-top: 15px;
            border:1px solid #d4d4d4;
            .tableInfo{
                padding: 8px;
                img{
                    width: 32px;
                    margin: 0 12px 0 35px;
                }
                .color1{
                    color:@orange;
                }
            }
            table{
                width:100%;
                text-align: center;
                th,td{
                    height: 30px;
                    border-bottom:solid 1px #d4d4d4;
                }
                tr>th{
                    width:16%;
                }
            }
            .tableFoot{
                padding:0 10px;
                height: 30px;
                line-height: 30px;
                p:first-child{
                    margin-right: 30px;
                }
            }
        }
        .title{
            font-size: 14px;
            color:#000;
        }

    }
    .isRight{
        padding:10px 20px 0 40px ;
        .newNotice{
            img{
                margin-right: 10px;
            }
            .DynamicList{
                margin-top: 10px;
                border-top: 1px solid #d4d4d4;
                padding-top: 10px;
                li{
                    font-size: 12px;
                    margin-bottom: 10px;
                    .listState{
                        display: inline-block;
                        width: 110px;
                        margin-left: 20px;
                    }
                }
                .state{
                    margin-left: 10px;
                    p:first-child{
                        margin-bottom: 10px;
                    }
                    .listNum{
                        white-space:nowrap;
                        overflow:hidden;
                        text-overflow:ellipsis;
                        width: 200px;
                    }
                }
            }
            .noticeList{
                margin-top: 10px;
                border-top: 1px solid #d4d4d4;
                padding-top: 10px;
                li{
                    font-size: 12px;
                    width: 80%;
                    display:block;
                    white-space:nowrap;
                    overflow:hidden;
                    text-overflow:ellipsis;
                    margin: 15px 0;
                }
            }
        }
        .newNotice2{
            padding-top: 20px;
            overflow: hidden;
            width:100%;
        }
        .greenHand{
            margin-top: 20px;
            .greenHandImg{
                margin-top: 10px;
                border-top: 1px solid #d4d4d4;
                padding-top: 10px;
                img{
                    width:100%;
                }
            }

        }
        .noticeButton{
            margin-top: 10px;
            font-size: 14px;
            a{
                color:@blue;
            }
        }
        .title{
            font-size: 16px;
            font-weight: 600;
        }
    }

}
