.orderFinancial{
    margin:0 30px;
    background: #fff;
    padding: 20px 30px;
    color:#333;
    table{
        margin:10px  0;
        color:#333;
        border: 1px solid #ddd;
        tr{
            height:45px;
        }
        th{
            font-size: 14px;
            background: #eee;
            text-align: left;
            font-weight: 500;
            padding-left: 20px;
            width:25%;
            span{
                font-size: 12px;
                color:#999;
                margin-left: 10px;
            }
        }
        td{
            font-size: 12px;
            padding-left: 20px;
        }
        .detailImage{
            padding-top: 10px;
            img{
                width:110px;
                border:1px solid #ddd;
                margin:15px;
            }
        }
    }
}
