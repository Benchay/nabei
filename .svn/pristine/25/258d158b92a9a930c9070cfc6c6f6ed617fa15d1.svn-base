.explain_text{
    margin-top: 20px;
    text-align: center;
    font-size: 20px;
    color: #333;
}
.role_content{
    margin:2px 30px;
    background: #fff;
    padding: 20px 40px;
    height:100%;
    overflow: auto;
}
.dialog_time{
    border-bottom:1px solid #bfcbd9;
    padding-bottom:20px;
}
.dialog_tit{
    height:20px;
    font-size: 14px;
    font-weight: 700;
    color:#000;
}
.dialogSize{
}
.dialog_jurisdiction{
    width:300px;
    margin: 10px 0;
}
.dialog_tag{
    margin-top: 15px;
}
.dialog_tags{
    margin-right: 3px;
    margin-bottom: 3px;
}
.tag_empty{
    color:#20a0ff;
    margin: 20px 0;
    display: block;
}
.export_excle{
    margin: 10px 0;
    margin-left: 10px;
}
.role-menu{
    background: #f4f4f4;
    height:50px;
    margin-left: 30px;
}
.role-menu>li>a{
    display: inline-block;
    line-height: 50px;
    width:160px;
    text-align: center;
    font-size: 16px;
    float: left;
    color:#333;
}
.role-menu>li:first-child{
    background: #da444f;
}
.role-menu>li:first-child a{
    background: #da444f;
    color:#fff;
}
.role-search{
    width:200px;
}
.addInput{
    margin-left: 22px;
    width: 110px;
}
.roleTable{
    margin-top: 50px;
}
.smallText{
    font-size: 12px;
    color:#999;
}
.el-tree{
    border:none;
}
.el-tree-node__content:hover {
    background:#fff;
}
