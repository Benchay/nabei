            confirmed(){
              this.compId = this.radio;
              this.dialogVisible = false;
              this.supplierName = '';
              for(var i = 0 ; i < this.supplierList.length;i++){
                    if(this.radio==this.supplierList[i].id){
                        this.name = this.supplierList[i].name;
                        return;
                    }
                }
                this.supplierList = [];
            },
