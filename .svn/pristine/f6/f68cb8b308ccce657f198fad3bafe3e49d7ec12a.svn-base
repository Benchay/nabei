			async exportFaileExcel() {		//导出excel
            	if(this.importFaileData.length>0){
            		const tHeader = ['公司名称', '档口编码','联系电话','地址','备注','失败原因'];
			　　　  const filterVal = ['name', 'stallCode','mobile','address','remark','msg'];
