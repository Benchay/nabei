.accountSetting {
    color: #333;
    margin: 1px 30px;
    background: #fff;
    height: 660px;
    overflow: auto;
    .bindingMobile {
        font-size: 14px;
        margin: 10% 0 0 30%;
        .tit {
            width: 120px;
            text-align: right;
            margin-right: 30px;
            span {
                color: @red;
            }
        }
        .box {
            display: flex;
            justify-content: flex-start;
            span {
                width: 100px;
                margin-top: 5px;
                span {
                    color: @red;
                }
            }
            input {
                width: 250px;
            }
        }
        div > div {
            margin-bottom: 12px;
        }
        .bindingButton {
            margin-top: 20px;
            margin-left: 190px;
        }
        .bindingFoot {
            margin-top: 20px;
            .title {
                color: @orange;
                margin-left: 5px;
                font-size: 12px;
            }
            p {
                margin-left: 16px;
                margin-top: 5px;
                font-size: 12px;
            }
        }
    }
}
.back{
    a{
        color:#333;
    }
}
.back:hover a{
    color:@blue;
}
