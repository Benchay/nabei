        }
    }
}
.dialogCD{
    height: 120px;
    p{
        font-size: 22px;
        margin-left: 20px;
    }
}
.yjcgFoot{
    margin-top: 20px;
    font-size: 12px;
    color:#333;
    p{
        margin-right: 40px;
    }
}
.yjcgDialog{
    flex-direction: column;
