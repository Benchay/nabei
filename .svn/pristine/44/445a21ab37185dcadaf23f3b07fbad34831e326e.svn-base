.startProcurement{
    min-height: 100%;
    margin:1px 30px;
    background: #fff;
    padding: 20px 40px;
    color:#333;
    .salesButton>button{
        width:88px;
        margin-bottom: 20px;
    }
    .salesDemandTop{
        margin-bottom: 20px;
    }
    .markOrder{
        color:@blue;
        font-size: 12px;
        line-height: 36px;
    }
    /*网店订单*/
    .salesOrderTable{
        padding-top: 10px;
        color:#333;
        .headImg{
            width:40px;
            height:40px;
        }
        .tableFirst{
            height: 40px;
            line-height: 40px;
            font-size: 14px;
            text-align: center;
            border:1px solid #d4d4d4;
            td{
                width:17%;
            }
            .firstLi{
                text-align: left;
                padding-left: 18px;
            }
            .salesCheckbox{
                width:20px;
                padding-left: 17px;
            }
        }
        .buyTable,.buyTable1,.buyTable2{
            border:1px solid #d4d4d4;
            border-bottom: none;
            font-size: 12px;
            .buyImg1{
                height: 40px;
                margin: 0 30px 0 10px;
            }
            th,td{
                height: 30px;
                line-height: 30px;
            }
            td{
                border-top: 1px solid #d4d4d4;
                height: 50px;
                line-height: 50px;
                text-align: center;
            }
            th{
                width:17%;
                font-weight: 100;
                background: #f4f4f4;
            }
            td{
                width:17%;
            }
            td:first-child{
                width:34%;
            }
            th:first-child{
                text-align: left;
                padding-left: 18px;
            }
            .tdFirst{
                display: flex;
                align-items: center;
                justify-content: flex-start;
                text-align: left;
                padding-right: 20px;
                p{
                    height: 30px;
                    line-height: 30px;
                }
            }
            .tdLast{
                p{
                    height: 20px;
                    line-height: 20px;
                    margin: 5px 0;
                }
                a{
                    margin: 0 5px;
                }
            }
            .checkbox{
                margin-left: 5px;
            }
            .unfinisdhedFirst{
                width:100%;
            }
        }
        .buyTable2{
            border-top: none;
            td:first-child{
                text-align: left;
                padding-left: 18px;
                width:17%;
            }
            td{
                height: 30px;
                line-height: 30px;
            }
        }
        .tableContent{
            border-bottom:1px solid #d4d4d4;
            // overflow: scroll;
            overflow-x: hidden;
        }
        .tableContent2{
            border-bottom:1px solid #d4d4d4;
            // overflow: scroll;
            overflow-x: hidden;
            // max-height: 165px;
            margin-bottom: 5px;
        }
        .nth-2{
            text-align: left;
        }
        .scrollTable{
            overflow: scroll;
            max-height: 410px;
        }
        .tableHead{
            margin-top: 5px;
        }
    }
    .salesDemandTable{
        .headImg{
            width:40px;
            height:40px;
        }
    }
    .export_excle{
        margin: 10px 0;
        margin-left: 10px;
    }
    .tendencyImg{
        width:20px;
        height:20px;
    }
    .purchaseList{
        li{
            float: left;
            font-size: 12px;
            margin-right: 10px;
            padding: 3px 8px;
            color:#333;
        }
        .tabss>button{
            background:@blue;
            color:#fff;
        }
    }
    .purchaseTable{
        padding-top: 40px;
        .purTable{
            width:100%;
            background: #f4f4f4;
            border:1px solid #d7d7d7;
            margin-bottom: 15px;
            .purTableTit{
                display:flex;
                justify-content: space-between;
                padding:0 30px;
                border-bottom:1px solid #d7d7d7;
                li{
                    display: flex;
                    align-items: center;
                    font-size: 14px;
                    height:36px;
                    line-height: 36px;
                    img{
                        margin-right: 5px;
                    }
                }
            }
            .purTableContent{
                background: #fff;
                font-size:14px;
                img{
                    width:66px;
                    height:66px;
                    border-radius: 50%;
                    float: left;
                    margin: 0 60px 0 40px;

                }
                li{
                    width:28%;
                    height:80px;
                    display: flex;
                    align-items: center;
                    border-left: 1px solid #d7d7d7;
                    text-align: center;
                    justify-content:center;
                }
                li:first-child{
                    width:44%;
                    border:none;
                    justify-content: flex-start;
                    .salesLists{
                        margin-right: 126px;
                        color:#333;
                    }
                    .salesLists:hover{
                        color:@red;
                    }
                }
                li:nth-child(2){
                    border:none;
                }
                li:last-child{
                    flex-direction: column;
                }
                .salesView{
                    color:@blue;
                }
                .salesMoney{
                    color:@orange;
                }
            }
        }
        .color1{
            color:@red;
        }
        .color2{
            color:#333;
        }
         }
    .oneKey{
        margin-left: 20px;
    }
    .salesOrderTop{
        height: 45px;
        .topSearch>div {
            width: 200px;
            margin-right: 10px;
        }
    }
    .orderTop{
        width:100%;
        p{
            font-size: 12px;
        }
        .uploatfile{
            position: relative;
            .buttonColor3{

            }
            #uploatfile{
                position: absolute;
                top:0;
                width:93px;
                height: 30px;
                opacity: 0;
            }
        }
        .buttonColor3:hover{
            color:rgb(209, 219, 229);
        }
    }
    .productTabs{
        margin-top: 20px;
    }
}
