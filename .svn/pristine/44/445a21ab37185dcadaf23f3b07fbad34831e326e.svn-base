.orderTable{
    padding-top: 40px;
}

//包裹动态
.dynamic>div{
    margin: 0 0 10px;
}
.dynamicList{
    min-height: 320px;
    margin-top: 30px;
    .track-rcol{width: 100%; border: 1px solid #eee;padding-bottom: 20px;}
    .track-list{margin: 20px; padding-left: 5px; position: relative;}
    .track-list li{
        position: relative; padding: 9px 0 0 25px; line-height: 18px; border-left: 1px solid #d9d9d9; color: #999;
        .node-icon{position: absolute; left: -6px; top: 50%; width: 11px; height: 11px;
            background: url(../image/yuan.png) no-repeat;background-size:100%; }
        .time{margin-right: 20px; position: relative; top: 4px; display: inline-block; vertical-align: middle;}
        .txt{max-width: 500px;margin-left: 80px;position: relative; top: 20px; display: inline-block; vertical-align: middle;font-size: 12px}
    }
    .track-list li:first-child{
        color: red; padding-top: 0; border-left-color: #fff;
        .time{margin-right: 20px; }
        .txt{max-width: 500px; }
        .node-icon{position: absolute; left: -6px; top: 50%; width: 11px; height: 11px;
            background: url(../image/yuan2.png) no-repeat;background-size:100%;}
    }
}
//新需求弹窗
.newRequirement{
    padding-right: 30px;
    .newList{
        margin-bottom: 20px;
        .title {
            text-align: right;
            color: #333;
        }
        .content {
            margin-left: 10px;
        }
        .newRequirementImg{
            width: 125px;
            height:125px;
            border:1px solid #d4d6d5;
        }
    }
}
//更换档口弹窗
.changeStall{
    .changeStallRadio{
        border-top: 1px solid #d4d6d5;
        border-bottom: 1px solid #d4d6d5;
        max-height: 400px;
        overflow: scroll;
        display: flex;
        flex-direction: column;
        padding: 15px 0 15px 30%;
        .el-radio__input.is-checked .el-radio__inner {
            border-color:@red;
            background:@red;
        }
        .el-radio__inner:hover {
            border-color: @red;
        }
    }
    .changeStallRadio>label{
        margin-bottom: 10px;
    }
}
