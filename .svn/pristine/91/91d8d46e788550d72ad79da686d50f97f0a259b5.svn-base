.sessionMenu{
    height:50px;
    margin-left: 30px;
    li{
        a{
            line-height: 50px;
            text-align: center;
            color:#333;
            width:160px;
            display: inline-block;
            float: left;
        }
    }
    li:last-child{
        a{
            background:@red;
            font-size: 16px;
            color:#fff;
        }
    }
}
.sessionManagement {
    margin: 0 30px;
    background: #fff;
    padding: 20px 40px;
    .sessionTop{
        .dinanciaSearch{
            width: 200px;
        }
    }
    .sessionButton{
        margin: 20px 0;
    }
}
