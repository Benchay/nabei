.homeStall{
    color:#333;
    background: #fff;
    margin: 15px 30px;
    padding: 5px 14px;
    height:100%;
    overflow: auto;
    font-size: 12px;
    .isLeft{
        height:100%;
        .shortcut{
            font-size: 18px;
            margin: 15px 0;
            ul{
                height: 100px;
                //overflow-y: hidden;
                li{
                    width: 24%;
                    height: 100px;
                    border-radius: 5px;
                    background: red;
                    float: left;
                    margin-right: 1%;
                    img{
                        margin-right: 10px;
                    }
                    a{
                        color:#fff;
                        width:100%;
                        height:100%;
                    }
                }
                li:hover{
                    box-shadow: 1px 1px 4px #ccc;
                }
                li:first-child{
                    background: #46cfc5;
                }
                li:nth-child(2){
                    background: #ff6c60;
                }
                li:nth-child(3){
                    background: #4ac4f1;
                }
                li:nth-child(4){
                    background: #f4c100;
                }
                li:nth-child(5){
                    background: #e683ff;
                }
                li:nth-child(6){
                    background: #83cc2b;
                }
                li:nth-child(7){
                    background: #fca393;
                }

            }
            .shortcutFirst{
                width: 100%;
                height: 45px;
                position: relative;
                .firstSelect{
                    width: 440px;
                    max-height: 60px;
                    position: absolute;
                    top:0;right:0;
                }
            }
        }
        .stallStatistics{
            height: 140px;
            margin-top: 15px;
            border:1px solid #d7d7d7;
            box-shadow: 1px 1px 3px 1px rgba(153,153,153,.3);
            padding:5px 15px;
            .stallTitle{
                height: 25px;
                width:100%;
                border-bottom: 1px solid #d7d7d7;
                a{
                    height: 25px;
                    line-height: 25px;
                    color:#333;
                    margin-right: 10px;
                }
                img{
                    width: 20px;
                    height: 20px;
                }
                .change3{
                    color:@blue2;
                }
            }
            ul{
                padding: 0 40px;
                justify-content: space-between;
                li{
                    height:100px;
                    display: flex;
                    align-items: center;
                    justify-content: center;
                    .tit{
                        font-size: 16px;
                        font-weight: 600;
                    }
                    p{
                        color:#333;
                        font-size: 14px;
                        height: 40px;
                        line-height: 40px;
                    }
                    img{
                        margin-right: 20px;
                    }
                }
            }
        }
        .stallData{
            margin-top: 15px;
            border:1px solid #d7d7d7;
            box-shadow: 1px 1px 3px 1px rgba(153,153,153,.3);
            padding:5px 15px;
            .stallDataTitle{
                height: 30px;
                width:100%;
                border-bottom: 1px solid #d7d7d7;
                p{
                    height: 25px;
                    line-height: 25px;
                }
                img{
                    width: 20px;
                    height: 20px;
                }
            }
        }
        .stallDataButton{
            ul{
                padding-left: 80px;
                margin-top: 10px;
                li{
                    width: 80px;
                    height: 24px;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    border:1px solid #ccc;
                    margin-right: 8px;
                    border-radius: 5px;
                    a{
                        color:#333;
                    }
                }
                .change2{
                    border:1px solid @blue2;
                    a{
                        color:@blue2;
                    }
                }
            }
        }
    }
    .isRight{
        padding:15px 20px 0 40px ;
        .newNotice{
            border:1px solid #d7d7d7;
            box-shadow: 1px 1px 3px 1px rgba(153,153,153,.3);
            padding: 15px 15px 40px 15px;
            img{
                margin-right: 10px;
            }
            .DynamicList{
                margin-top: 10px;
                border-top: 1px solid #d4d4d4;
                padding-top: 10px;
                li{
                    font-size: 12px;
                    margin-bottom: 10px;
                    .listState{
                        display: inline-block;
                        width: 110px;
                        margin-left: 20px;
                    }
                }
                .state{
                    margin-left: 10px;
                    p:first-child{
                        margin-bottom: 5px;
                    }
                    .listNum{
                        white-space:nowrap;
                        overflow:hidden;
                        text-overflow:ellipsis;
                        width: 180px;
                    }
                }
            }
            .noticeList{
                margin-top: 10px;
                border-top: 1px solid #d4d4d4;
                padding-top: 10px;
                li{
                    font-size: 12px;
                    width: 80%;
                    display:block;
                    white-space:nowrap;
                    overflow:hidden;
                    text-overflow:ellipsis;
                    margin: 15px 0;
                }
            }
        }
        .newNotice2{
            margin-top: 15px;
            overflow: hidden;
            padding: 15px;
        }
        .greenHand{
            margin-top: 15px;
            display: block;
            border:1px solid #d7d7d7;
            box-shadow: 1px 1px 3px 1px rgba(153,153,153,.3);
            padding: 15px;
            color:#333;
            .greenHandImg{
                margin-top: 10px;
                border-top: 1px solid #d4d4d4;
                padding-top: 10px;
                img{
                    width:100%;
                }
            }

        }
        .noticeButton{
            margin-top: 10px;
            font-size: 14px;
            a{
                color:@blue;
            }
        }
        .title{
            font-size: 16px;
            font-weight: 600;
        }
    }

}
