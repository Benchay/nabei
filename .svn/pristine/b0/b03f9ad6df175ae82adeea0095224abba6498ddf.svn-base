.MSConfig{
    color:#666;
    font-size: 14px;
    margin:2px 30px;
    background: #fff;
    padding: 1px  30px;
    height:100%;
    overflow: auto;
    //修改切换页
    .el-tabs__item.is-active {
        color: @blue2;
    }
    .el-tabs__active-bar {
        position: absolute;
        bottom: 0;
        left: 0;
        height: 3px;
        background-color: @blue2;
        z-index: 1;
        transition: transform .3s cubic-bezier(.645,.045,.355,1);
        list-style: none;
    }
    .el-tabs__header {
        border-bottom: none;
        padding: 0;
        position: relative;
        margin: 0 0 15px;
    }
//切换内容
    .firstContent{
        padding-bottom: 20px;
        a{
            display: inline-block;
        }
        .FitstList{
            margin-top: 20px;
            img{
                width: 36px;
                height: 36px;
                padding-top: 5px;
            }
            .trefresh>img {
                width: 25px;
                height: 25px;
            }
            .delete{
                margin-top: 20px;
                font-size: 12px;
            }
            .viewDetails{
                color:@blue;
            }
        }
    }
    .secondContent{
        height: 200px;
        color:#666;
        .secondTop{
            margin-top: 25px;
            p{
                width: 120px;
            }
            .secondInput{
                width: 120px;
                text-align: center;
            }
            span{
                margin: 0 10px;
            }
        }
        .secondButton{
            margin-top: 50px;
            a{
                width: 120px;
                text-align: center;
                margin: 0 180px;
            }
        }
    }
    .thirdContent{

    }
    .secondTips{
        margin-top: 50px
    }
    .secondTips>div>div{
        height: 36px;
    }
    .MCtips{
        color:@orange;
        font-size: 12px;
    }
}
