.tc{
    .el-input__inner{
        text-align: center;
        &:disabled{
            color: #666666;
            background-color: #f4f4f4;
        }
    }
}
.goodscontent{
    display: flex;
    padding: 0 50px;
    margin: 50px 20px;
    .sidegood{
        width: 80px;
        &:first-child{
            margin-right: 35px;
        }
        &:last-child{
            margin-left: 35px;
        }
        .sideimgwarp{
            width: 80px;
            height: 80px;
            border-radius: 50%;
        }
        p{
            text-align: center;
            margin-top: 15px;
        }
    }
    .gooditem{
        flex: 1;
        min-width: 226px;
        font-size: 0;
        margin-right: 10px;
        text-align: center;
        padding-top: 25px;
        .mdimgwrap{
            display: inline-block;
            vertical-align: top;
            width: 40px;
            height: 40px;
            border-radius: 50%;
            box-sizing: border-box;
            border: 1px solid #dcdcdc;
            margin-right: 10px;
            cursor: pointer;
            position: relative;
            img{
                position: absolute;
                top: -1px;
                left: -1px;
                width: 40px;
                height: 40px;
                border-radius: 50%;
            }
        }
        .describetion{
            font-size: 14px;
            display: inline-block;
            vertical-align: top;
            margin-top: 9px;
        }
        .line{
            display: inline-block;
            vertical-align: top;
            margin-top: 16px;
            width: 110px;
            margin-left: 10px;
            box-sizing: border-box;
            border-bottom: 1px dashed #dcdddc;
        }
        .active{
            border-color: #da444f;
        }
    }
    .lastitem{
        width: 113px;
        padding-top: 25px;
           .mdimgwrap{
            display: inline-block;
            vertical-align: top;
            width: 40px;
            height: 40px;
            box-sizing: border-box;
            border-radius: 50%;
            border: 1px solid #dcdcdc;
            margin-right: 10px;
            cursor: pointer;
            position: relative;
            img{
                position: absolute;
                top: -1px;
                left: -1px;
                width: 40px;
                height: 40px;
                border-radius: 50%;
            }
        }
        .describetion{
            font-size: 14px;
            display: inline-block;
            vertical-align: top;
            margin-top: 9px;
        }
    }
}
