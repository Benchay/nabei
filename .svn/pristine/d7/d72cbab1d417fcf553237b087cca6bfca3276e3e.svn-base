.inventoryInitialize{
    margin:0 30px;
    background: #fff;
    padding: 20px 40px;
    height:100%;
    .initializeTop{
        input{
            width: 200px;
            margin-right: 10px;
        }
    }
    .initializeButton{
        padding: 20px 0;
        overflow: hidden;
        width:100%;
    }
    .initializeTable{
        img{
            width: 40px;
            height: 40px;
        }
    }
}
//添加新商品 弹窗
.newCommodity{
    padding:0 30px;
    .avatar-uploader .el-upload {
        border: 1px dashed #d9d9d9;
        border-radius: 6px;
        cursor: pointer;
        position: relative;
        overflow: hidden;
    }
    .avatar-uploader .el-upload:hover {
        border-color: #20a0ff;
    }
    .avatar-uploader-icon {
        font-size: 28px;
        color: #8c939d;
        width: 128px;
        height: 128px;
        line-height: 128px;
        text-align: center;
    }
    .avatar {
        width: 128px;
        height: 128px;
        display: block;
    }
    .newInput{
        width: 300px;
    }
}
