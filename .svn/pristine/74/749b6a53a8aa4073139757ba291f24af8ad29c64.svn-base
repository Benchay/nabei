.basic-menu{
    background: #f4f4f4;
    height:50px;
    margin-left: 30px;
}
.basic-menu>li>a{
    display: inline-block;
    line-height: 50px;
    width:160px;
    text-align: center;
    font-size: 16px;
    float: left;
    color:#333;
}
.basic-menu>li:last-child a{
    background: #da444f;
    color:#fff;
}
.basic{
    margin:0 30px;
    background: #fff;
    padding: 20px 40px;
    height:100%;
    .changeImg{
        width: 78px;
        text-align: center;
        font-size: 12px;
        color:#333;
    }
    .basicInfo{
        font-size: 14px;
        color:#333;
        div{
            height: 30px;
            line-height: 30px;
            display: flex;
            align-items: center;
            span{
                //margin-left: 5px;
            }
            .title{
                width: 90px;
                text-align: left;
            }
        }
    }
    .changepwd{
        text-decoration: underline;
        color:@blue;
        font-size: 12px;
    }
    .basicBorder{
        padding-bottom: 10px;
        border-bottom: 1px solid #f4f4f4;
    }
    .creationInfo{
        margin-top: 20px;
        p{
            color:@blue;
        }
        div{
            margin-left: 85px;
        }
    }
    .basicTable{
        margin-top: 20px;
        padding-bottom: 50px;
        img{
            width: 40px;
            height: 40px;
        }
    }
}
.avatar-uploader .el-upload {
    border: 1px dashed #d9d9d9;
    border-radius: 6px;
    cursor: pointer;
    position: relative;
    overflow: hidden;
}
.avatar-uploader .el-upload:hover {
    border-color: #20a0ff;
}
.avatar-uploader-icon {
    font-size: 28px;
    color: #8c939d;
    width: 78px;
    height: 78px;
    line-height: 78px;
    text-align: center;
}
.avatar {
    width: 78px;
    height: 78px;
    display: block;
}
