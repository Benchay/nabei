.commodityMnagement{
    margin:0 30px;
    background: #fff;
    padding: 20px 40px;

    .commodityList{
        margin:50px 0;
        .tableFirst{
            padding: 2px 0 ;
            display: flex;
            align-items: center;
            justify-content: flex-start;
            div{
                width: 18px;
                height: 18px;
            }
        }
        .masterDrawing{
            width: 40px;
            height: 40px;
            margin-left: 10px;
        }
        .trefresh>img {
            width: 25px;
            height: 25px;
        }
        .operation{
            img{
                border-right: 1px solid #ccc;
                padding: 0 10px;
            }
            img:last-child{
                border-right: none;
            }
        }
    }
    .commodityTop>div{
        width: 200px;
    }
    .commodityButton{
        width:100%;
        margin: 15px 0;
    }
}
.binding{
    background: #fff;
    margin:0 30px 20px;
    height:83%;
    flex-direction: column;
    img{
        width: 100px;
    }
    p{
        font-size: 13px;
        margin: 10px 0;
        color:#999;
    }
    p>a{
        font-size: 16px;
        border-bottom: 1px solid @blue;
        color:@blue;
    }
}
//修改弹窗内容
.settingRelevance{
    color:@red;
    margin: 20px 0 5px;
}
.dialog-footer{
    margin-top: 20px;
    padding-top: 20px;
    display: flex;
    align-items: center;
    justify-content: center;
    width:100%;
    border-top: 1px solid #f4f4f4;
}
//添加新商品弹窗
.dialogAdd{
    height: 655px;
    .addTitle{
        margin-bottom: 20px;
    }
    .DialogTitle{
        width: 100px;
        text-align: right;
        margin-right: 10px;
    }
    .choose{
        border: none;
        color:@red;
        margin-left: 5px;
        font-size: 12px;
        border-bottom: 1px solid @red;
        border-radius: 0;
    }
    .addImg{
        width: 125px;
    }
}
.el-dialog {margin-bottom: 0;}
.continueAdd{
    font-size: 12px;
    color:@red;
    margin-top: 5px;
}
.chooseRadio{
    font-size: 12px;
    margin-top: 15px;
    display: flex;
    flex-direction: column;
    max-height: 200px;
    overflow: scroll;
    padding-left: 30px;
}
//new
.dialogAdd2{
    .dialogSearch{
        margin-top: 20px;
        .search1{
            width: 300px;
        }
    }
    .dialogTable2{
        margin-top: 20px;
    }
}
