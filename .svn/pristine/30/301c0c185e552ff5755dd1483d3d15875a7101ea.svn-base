                    let data = res.result.results;
                    this.supplierList.push({id:'',name:'匿名卖家'});
                    for(var i = 0 ; i < data.length;i++){
                        let pa= {
                            recordId:data[i].id,
                            id:data[i].sellId,
                            name:data[i].sellName
                        };
                        this.supplierList.push(pa);
