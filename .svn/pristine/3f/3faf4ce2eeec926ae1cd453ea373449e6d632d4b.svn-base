.homeStall{
    color:#333;
    background: #fff;
    margin: 15px 30px;
    padding: 10px 14px;
    height:100%;
    font-size: 12px;
    .isLeft{
        .shortcut{
            font-size: 24px;
            margin-top: 5px;
            border:1px solid #d7d7d7;
            box-shadow: 1px 1px 3px 1px rgba(153,153,153,.3);
            padding: 15px;
            ul{
                display: flex;
                justify-content: space-between;
                padding: 0 40px;
                li{
                    width: 300px;
                    height: 120px;
                    border-radius: 5px;
                    img{
                        margin-right: 35px;
                    }
                    a{
                        color:#fff;
                        width:100%;
                        height:100%;
                    }
                }
                li:hover{
                    box-shadow: 1px 1px 4px #ccc;
                }
                li:first-child{
                    background: #7ec857;
                }
                li:last-child{
                    background: #5d6977;
                }

            }
        }
        .stallData{
            margin-top: 20px;
            border:1px solid #d7d7d7;
            box-shadow: 1px 1px 3px 1px rgba(153,153,153,.3);
            padding:5px 15px;
            .stallDataTitle{
                height: 25px;
                width:100%;
                border-bottom: 1px solid #d7d7d7;
                p{
                    height: 25px;
                    line-height: 25px;
                }
                img{
                    width: 20px;
                    height: 20px;
                }
            }
        }
        .stallDataButton{
            ul{
                padding-left: 80px;
                margin-top: 20px;
                li{
                    width: 80px;
                    height: 28px;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    border:1px solid #ccc;
                    margin-right: 8px;
                    border-radius: 5px;
                    a{
                        color:#333;
                    }
                }
                .change2{
                    border:1px solid @blue2;
                    a{
                        color:@blue2;
                    }
                }
            }
        }
    }
    .isRight{
        padding:5px 20px 0 40px ;
        .newNotice{
            border:1px solid #d7d7d7;
            box-shadow: 1px 1px 3px 1px rgba(153,153,153,.3);
            padding: 15px 15px 40px 15px;
            img{
                margin-right: 10px;
            }
            .DynamicList{
                margin-top: 10px;
                border-top: 1px solid #d4d4d4;
                padding-top: 10px;
                li{
                    font-size: 12px;
                    margin-bottom: 10px;
                    .listState{
                        display: inline-block;
                        width: 110px;
                        margin-left: 20px;
                    }
                }
                .state{
                    margin-left: 10px;
                    p:first-child{
                        margin-bottom: 5px;
                    }
                    .listNum{
                        white-space:nowrap;
                        overflow:hidden;
                        text-overflow:ellipsis;
                        width: 180px;
                    }
                }
            }
            .noticeList{
                margin-top: 10px;
                border-top: 1px solid #d4d4d4;
                padding-top: 10px;
                li{
                    font-size: 12px;
                    width: 80%;
                    display:block;
                    white-space:nowrap;
                    overflow:hidden;
                    text-overflow:ellipsis;
                    margin: 15px 0;
                }
            }
        }
        .newNotice2{
            margin-top: 10px;
            overflow: hidden;
            padding: 15px;
        }
        .greenHand{
            margin-top: 10px;
            display: block;
            border:1px solid #d7d7d7;
            box-shadow: 1px 1px 3px 1px rgba(153,153,153,.3);
            padding: 15px;
            color:#333;
            .greenHandImg{
                margin-top: 10px;
                border-top: 1px solid #d4d4d4;
                padding-top: 10px;
                img{
                    width:100%;
                }
            }

        }
        .noticeButton{
            margin-top: 10px;
            font-size: 14px;
            a{
                color:@blue;
            }
        }
        .title{
            font-size: 16px;
            font-weight: 600;
        }
    }

}
