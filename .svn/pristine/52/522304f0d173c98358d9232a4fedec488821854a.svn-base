    }
    .el-radio-button__inner:hover{
        color:@blue2;
    }
    .el-radio__input.is-checked .el-radio__inner {
        border-color:@blue2;
        background:@blue2;
    }
    .el-radio__inner:hover {
        border-color: @blue2;
    }
}
