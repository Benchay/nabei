.staff-batch{
    width:110px;
    margin-top: 10px;
}

.staff_table{
    padding: 10px 0;
}
.add-dialog{
    width:300px;
}
.dialog-a{
    color:#20a0ff;
    margin-left: 10px;
}
.staff-menu{
    background: #f4f4f4;
    height:50px;
    margin-left: 30px;
}
.staff-menu>li>a{
    display: inline-block;
    line-height: 50px;
    width:160px;
    text-align: center;
    font-size: 16px;
    float: left;
}
.staff-menu>li>a{
    color:#333;
}
.staff-menu>li:nth-child(2){
    background: #da444f;
}
.staff-menu>li:nth-child(2) a{
    background: #da444f;
    color:#fff;
}
.staff-content{
    margin:0 30px;
    background: #fff;
    padding: 20px 40px;
    height:100%;
    .addradio{
        margin: 20px 0;
    }
    .YGlist{
        margin-top: 20px;
        img{
            width:40px;
            height: 40px;
            border-radius: 50%;
        }
        .YGimg{
            text-align: right;
            margin-right: 10px;
        }
    }
}
.staff-status{
    width:110px;
    margin-right: 40px;
}
.staffCheckbox{
    .el-checkbox-group{
        .el-checkbox:first-child{
            margin-left: 15px;
        }
    }
    a{
        color:@red;
    }
}
.addStaff{
    margin-top: 10px;
    width:110px;
}
