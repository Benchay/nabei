				LODOP.SET_PRINT_STYLEA(0, "bold", 1);

				LODOP.ADD_PRINT_BARCODE(50,20,300,50,"Code39",barCode); 

				LODOP.ADD_PRINT_TEXT(120,20,500,30,detail)
