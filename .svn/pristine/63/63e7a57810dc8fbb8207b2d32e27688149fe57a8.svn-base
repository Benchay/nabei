.addShop{
    margin:1px 30px;
    background: #fff;
    padding: 20px 40px;
    height:100%; 
        .red{
        cursor: pointer;
    }
        .toptitle{
            font-size: 14px;
        }  
        .selectad{
            width: 140px;
        }
        .addshopbtn{
            height: 30px;
            &:focus{
                color: #fff;
            }
            &:hover{
                color: #fff;
            }
        } 
        .tip{
            font-size: 14px;
            white-space: nowrap;
            margin: 20px 0 30px 0;
        } 
        .havenoshop{
            font-size: 0;
            text-align: center;
            height: 100px;
            box-sizing: border-box;
            border: 1px solid #d7d7d7;
            background: #f4f4f4;
            padding: 28px 0;
        }
        .imgwrap{
            display: inline-block;
            width: 50px;
            height: 44px;
            vertical-align: top;
            margin-right: 20px;
        } 
        .nstitle{
            display: inline-block;
            font-size: 16px;
            vertical-align: top;
            color: #999999;
            font-weight: bold;
            margin-top: 10px;
        } 
        .middletitle{
            margin-top: 30px;
            margin-bottom: 12px;
        }
        .bannerwrap{
            padding-top: 15px;
            width: 100%;
            height: 350px;
            margin: 0 auto;
            box-sizing: border-box;
            border: 1px solid #d7d7d7;
            .lunwrap{
                width: 100%;
                height: 350px;
                padding: 0 10%
            }
        }
}

