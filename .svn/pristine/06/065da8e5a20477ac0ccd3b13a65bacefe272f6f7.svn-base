.papa{
    margin:2px 30px;
    background: #fff;
    padding: 50px 10px;
    font-size: 14px;
    color: #666666;
    display: flex;
    display: -webkit-box;
    display: -ms-flexbox;
    justify-content: center;
    align-items: flex-start;
    position: relative;
    .btngroup{
        display: block;
        position: absolute;
        top:5px;
        right: 10px;
        margin-top: 5px;
    }
    .paydetails{
        width: 360px;
        margin: 0 10px;
        padding-bottom: 20px;
        border: 1px solid #cdcdcd;
        box-shadow: 4px 4px 4px #dcdcdc;
        height: 539px;
        overflow-y: scroll;
        .paydetailsHeader{
            .title{
                text-align: center;
                color: #da444f;
                padding: 18px 0;
                i{
                    width: 24px;
                    height: 24px; 
                }
            }
            .failreason{
                padding: 10px;
                color: #ff7914;
                line-height: 1.6;
            }
        }
        .paydetailsBody{
            .paydetailsBodyTop{
                height: 32px;
                line-height: 32px;
                background-color: #f4f4f4;
                border-top: 1px solid #cdcdcd;
                border-bottom: 1px solid #cdcdcd; 
                padding-left: 10px;
            }
            .paydetailsBodyMain{
                padding: 0 10px;
                .flex{
                    justify-content: space-between;
                    margin-top: 20px;
                    .list{
                        .imgwrap{
                            float: right;
                            margin-left: 5px;
                            width: 64px;
                            height: 64px;
                            border: 1px solid #cccccc;
                        }
                    }
                }
            }
        }
    }
    .stalllist{
        flex: 1;
        margin: 0 10px;
        padding: 0 10px;
        border: 1px solid #cdcdcd;
        box-shadow: 4px 4px 4px #dcdcdc;
        height: 561px;
       .stalllistTitle{
            display: flex;
            justify-content: space-between;
            flex-wrap: nowrap;
            white-space: nowrap;
            padding: 20px 0;
            div{
                margin-right: 30px;
            }
       }
    }
}
.auditwarp{
    .el-textarea__inner{
        height: 140px;
    }
    .el-dialog__footer{
        text-align: center;
        a{
            cursor: pointer;
        }
    }
}