.manage_page{

}
.manage-company{
    margin-top: 10px;
    padding-left: 30px;
}
.manage-content{
    height:60px;
    width:100%;
    background:#fff;
    /*box-sizing: border-box;*/
}
.manage-top-menu>li{
    float: left;
    width:90px;
    height:60px;
    line-height:60px;
    margin-right: 15px;
}
.manage-logo{
    background: pink;
    height:60px;
    width:100%;
}
.topMenuLists,.hand-user{
    display: flex;
    align-items: center;
    color:#333;
    font-size: 14px;
    .handOut{
        margin-left: 10px;
    }
}
.topMenuLists>span{
    padding: 0 5px;
}
.avator{
    margin-right: 10px;
}
.icon{
    width:20px;
    height:20px;
    display: inline-block;
    background-repeat: no-repeat;
}
.icon-home{
    background-image: url("../image/lhomepage01.png");
}
.icon-procurement{
    background-image: url("../image/lprocurement01.png");
}
.icon-finance{
    background-image:url("../image/lfinance01.png") ;
}
.icon-commodity{
    background-image: url("../image/lgoods01.png");
}
.icon-linkman{
    background-image: url("../image/lcontacts01.png");
}
.icon-other{
    background-image: url("../image/lother01.png");
}
.icon-warehouse{
    background-image: url("../image/storehouse01.png");
}
.manage-menu{
    display: flex;
    align-items: center;
    width:80%;
    span{
        text-indent: 10px;
        a{
            color:rgb(191, 203, 217);
        }
    }
}

.iconRight{
    width: 22px;
    height:22px;
    background-image: url("../image/lshrink.png");
    margin-right: 15px;
}
.iconLeft{
    width: 22px;
    height:22px;
    background-image: url("../image/lstretch.png");
    margin-right:20px;
}
.button-left{
    width:100%;
    height:40px;
    padding-top: 10px;
}
.manageList{
    overflow:scroll;
    max-height: 690px;
}
