.salesReturn{
    margin:1px 30px;
    background: #fff;
    padding: 20px 40px;
    height:100%;
    overflow: auto;
    color:#333;
    .salesTop{
        font-size: 14px;
        .salesTopInput>div{
            width: 200px;
            margin-left:5px;
        }
    }
    .salesRadio{
        margin: 15px 0;
    }
    //表格内容
    .salesTable{
        img{
            width: 40px;
            height: 40px;
        }
        .manner{
            margin-top: 20px;
            font-size: 14px;
            a{
                color:@red;
                margin-right: 20px;
            }
            a>span{
                background-size: 100%;
                width: 36px;
                height: 36px;
                display: inline-block;
                margin-right: 5px;
            }
            .background1{
                background: url("../image/tedit04.png") no-repeat center;
            }
        }
    }
    .defeated{
        font-size: 13px;
        color:#333;
        border:1px solid #ddd;
        border-top:none;
        padding: 10px 50px 10px 20px ;
        ul>li{
            height: 28px;
            line-height: 28px;
        }

    }
}

//退货详情
