//确认退货
.returnsAffirm{
    color:#666;
    font-size: 14px;
    margin:1px 30px;
    background: #fff;
    padding: 20px 30px;
    height:100%;
    .affirmButtom{
        margin-bottom: 10px;
    }
    .returnsAffirmTable{
        padding-top: 10px;
        color:#333;
        .headImg{
            width:40px;
            height:40px;
        }
        .tableFirst{
            height: 40px;
            line-height: 40px;
            font-size: 14px;
            text-align: center;
            border:1px solid #d4d4d4;
            td{
                width:17%;
            }
            .firstLi{
                text-align: left;
                padding-left: 18px;
            }
            .salesCheckbox{
                width:20px;
                padding-left: 17px;
            }
        }
        .buyTable,.buyTable1,.buyTable2{
            border:1px solid #d4d4d4;
            border-bottom: none;
            font-size: 12px;
            .buyImg1{
                height: 40px;
                margin: 0 30px 0 10px;
            }
            th,td{
                height: 30px;
                line-height: 30px;
            }
            td{
                border-top: 1px solid #d4d4d4;
                height: 50px;
                line-height: 50px;
                text-align: center;
            }
            th{
                width:17%;
                font-weight: 100;
                background: #f4f4f4;
            }
            td{
                width:17%;
            }
            td:first-child{
                width:34%;
            }
            th:first-child{
                text-align: left;
                padding-left: 18px;
            }
            .tdFirst{
                display: flex;
                align-items: center;
                justify-content: flex-start;
                text-align: left;
                padding-right: 20px;
                p{
                    height: 30px;
                    line-height: 30px;
                }
            }
            .tdLast{
                p{
                    height: 20px;
                    line-height: 20px;
                    margin: 5px 0;
                }
                a{
                    margin: 0 5px;
                }
            }
            .checkbox{
                margin-left: 5px;
            }
            .unfinisdhedFirst{
                width:100%;
            }
        }
        .buyTable2{
            border-top: none;
            td:first-child{
                text-align: left;
                padding-left: 18px;
                width:17%;
            }
            td{
                height: 30px;
                line-height: 30px;
            }
            .remark{
                padding: 1px;
                width: 400px;
            }
        }
        .tableContent{
            border-bottom:1px solid #d4d4d4;
            overflow: scroll;
            overflow-x: hidden;
        }
        .tableContent2{
            border-bottom:1px solid #d4d4d4;
            overflow: scroll;
            overflow-x: hidden;
            max-height: 165px;
            margin-bottom: 5px;
        }
        .nth-2{
            text-align: left;
        }
        .scrollTable{
            overflow: scroll;
            max-height: 410px;
        }
        .tableHead{
            margin-top: 5px;
        }
    }
}
.returnsAffirmDialog{
    font-size: 16px;
    padding: 30px 0 40px;
    p{
        margin-left: 10px;
    }
}
