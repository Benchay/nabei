@loginBule:#1883e5;
.createCompany{
    color: #333;
    background: #fff;
    margin: 15px 30px;
    padding: 10px 14px;
    height: 88%;
    .createHead{
        margin: 30px 0 15px;
        height: 80px;
        p{
            font-size: 18px;
            margin-left: 10px;
        }
    }
    .createForm{
       .formInput{
           width: 310px;
       }
        .createFoot,.createFoot1{
            margin-top: 60px;
            padding-left: 80px;
            font-size: 15px;
            .confirm{
                width:95%;
                background: @loginBule;
                color:#fff;
                height: 36px;
                border-radius: 5px;
            }
            .back{
                width:95%;
                height: 34px;
                border-radius: 5px;
                border: 1px solid @loginBule;
                color:@loginBule;
            }
        }
        .createFoot1{
         margin-top: 20px;
        }
    }
    .joinSearch{
        .joinInput{
            width: 400px;
        }
        a{
            width: 70px;
            height: 36px;
            background: @loginBule;
            border-radius: 0 5px 5px 0;
            color:#fff;
            margin-left: -2px;
            z-index: 2;
            font-size: 14px;
        }
    }
    .joinTable{
        margin-top: 10px;
        img{
            width: 40px;
            height: 40px;
        }
        .searchNumber{
            font-size: 12px;
            margin-bottom: 5px;
        }
    }
}
.companyList{
    img{
        width: 50px;
        height: 50px;
        margin-right: 10px;
    }
}
.companyInfo{
    margin-left: 60px;
    p{
        height: 30px;
    }
}
.joinVerify{
    color:#666;
    margin: 20px 0;
    input{
        width: 250px;
    }
    span{
        width: 100px;
    }
}
.companyListFoot{
.confirm{
    background: @loginBule;
}
}
.joinFoot{
    margin-top: 30px;
    .confirm1{
        width: 380px;
        background: @loginBule;
        color:#fff;
        height: 36px;
        border-radius: 5px;
    }

}
