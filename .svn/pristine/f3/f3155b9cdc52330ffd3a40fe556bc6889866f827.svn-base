.stallIntroduction{
    color:#333;
    background: #fff;
    margin: 15px 30px;
    padding: 10px 14px;
    height:100%;
    font-size: 12px;
    .stallCarousel{
        padding: 0 10px;
        height: 660px;
        .el-carousel__container{
            height:630px;
        }
        .stallList{
            //background: pink;
            box-sizing: border-box;
            height: 630px;
            padding: 20px 30px 0;
            font-size: 16px;
            .listText{
                padding: 0 90px;
                height: 150px;
                .text2{
                   text-indent: 32px;
                }
                p{
                    line-height: 40px;
                }
            }
            .listImg{
                margin-top: 15px;
                padding: 0 120px;
                img{
                    width: 800px;
                    height: 400px;
                }
            }
        }
    }
}
